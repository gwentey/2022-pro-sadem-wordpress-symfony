<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1">
		<!-- CSS only -->

		<title>
			{% block title %}Sadem
			{% endblock %}
		</title>
		<link
		rel="icon" type="image/x-icon" href="{{ asset('images/front/favicon.ico') }}"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

		{% block stylesheets %}
			{{ encore_entry_link_tags('app-front-common') }}
			{{ encore_entry_link_tags('app-front') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app-front-common') }}
			{{ encore_entry_script_tags('app-front') }}
		{% endblock %}
	</head>

	<body>
		{% include 'Front/partials/_loading.html.twig' %}

		{% include 'Front/partials/_navigation.html.twig' %}

		{% block body %}{% endblock %}

	</body>

	{{ footer() }}
